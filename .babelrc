{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-3",
    "react"
  ],
  "plugins": [
    "transform-react-jsx",
    "transform-runtime",
    "transform-es2015-modules-commonjs",
    "syntax-dynamic-import",
    ["import",
      { 
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ]
  ]
}